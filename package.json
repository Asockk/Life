{
  "name": "blutdruck-tracker",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "homepage": "https://asockk.github.io/Life/",
  "description": "Blutdruck Tracker Application",
  "author": "Your Name <your-email@example.com>",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "crypto-js": "^4.2.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.263.1",
    "papaparse": "^5.4.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-swipeable": "^7.0.2",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.6.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder build --mac --win --linux",
    "electron-pack-mac": "electron-builder build --mac",
    "electron-pack-win": "electron-builder build --win",
    "electron-pack-linux": "electron-builder build --linux",
    "postinstall": "electron-builder install-app-deps",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^25.3.1",
    "electron-builder": "^24.4.0",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^3.3.2",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.blutdruck.tracker",
    "productName": "Blutdruck Tracker",
    "copyright": "Copyright Â© 2024 Your Name",
    "npmRebuild": true,
    "nodeVersion": "18.16.0",
    "mac": {
      "category": "public.app-category.medical",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Utility",
      "maintainer": "Your Name <your-email@example.com>"
    },
    "files": [
      "build/**/*",
      "electron.js",
      {
        "from": "build",
        "to": "build",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    }
  }
}
